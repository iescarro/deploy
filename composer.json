{
    "name": "iescarro/deploy",
    "description": "A very simple deployment tool",
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DeployPHP\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}